 
 //package - These are pre written code.  
 // These are imported using the keyword "import" 
 
 import java.util.ArrayList; import java.util.List;  
 
 //Name of the class and the file name should match. 
 //For example, here, name of the file should be MyFirstCode.java 
 //Class name is always starts with capital and camelcase 
 
 public class MyFirstCode {     
 
   //main method is where the program starts its execution     
 
   public static void main(String args[]){         
     List myList = new ArrayList<>();         
     myList.add(1);         
     myList.add(2);         
     myList.add(3);          
     for (Integer num : myList){             
       System.out.println(num);         }     } }
  
